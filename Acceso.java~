import java.util.Scanner;
public class Acceso
{
    private int año;
    private int mes;
    private int dia;
    private int hora;
    private int minutos;
    private Scanner sc;

    public Acceso(String fechaAcceso)
    {
        String fechaIntroducida = fechaAcceso;
        sc = new Scanner(fechaIntroducida);
        if(sc.hasNext()){
            String[] fechaAGuardar = sc.nextLine().split(" ");
            año = Integer.parseInt(fechaAGuardar[0]);
            mes = fechaAGuardar[1];
            dia = fechaAGuardar[2];
            hora = fechaAGuardar[3];
            minutos = fechaAGuardar[4];
        }
    }

    public String getAno() 
    {
        return año;
    }

    public String getMes()
    {
        return mes;
    }

    public String getDia()
    {
        return dia;
    }

    public String getHora()
    {
        return hora;
    }

    public String getMinutos()
    {
        return minutos;
    }
}